name: Build Geode Mod
on: [push, pull_request]
jobs:
  build:
    strategy:
      matrix:
        config:
        - name: Android64
          os: ubuntu-latest
          target: Android64
    name: ${{ matrix.config.name }}
    runs-on: ${{ matrix.config.os }}
    steps:
      - uses: actions/checkout@v4
      - name: Build Mod
        uses: geode-sdk/build-geode-mod@main
        with:
          target: ${{ matrix.config.target }}
          
